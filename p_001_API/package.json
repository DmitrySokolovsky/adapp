{
  "name": "p001-server",
  "version": "0.1.0",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "webpack --config webpack.config.js --env=dev",
    "prestart": "npm run build",
    "start": "nodemon node ./build/p001-server.app.bundle.js",
    "preprod": "npm test",
    "prod": "webpack --config webpack.config.js --env=prod",
    "prestart-prod": "npm run prod",
    "start-prod": "node ./build/p001-server.app.bundle.min.js",
    "test": "jest --config jest.config.json --forceExit",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/express": "^4.0.36",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^8.0.13",
    "@types/supertest": "^2.0.4",
    "awesome-typescript-loader": "^4.0.1",
    "clean-webpack-plugin": "^0.1.19",
    "jest": "^22.4.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.1",
    "ts-node": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "webpack": "^3.9.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "@types/pg": "^7.4.9",
    "express": "^4.16.2",
    "inversify": "^4.11.1",
    "inversify-express-utils": "^5.2.1",
    "inversify-logger-middleware": "^3.1.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.37.10",
    "sequelize-typescript": "^0.6.6-beta.1"
  }
}
